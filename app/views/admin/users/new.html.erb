<h1>Sign Up</h1>
<%= form_for([:admin, @user]) do |f| %>
  <%= render 'form', user: @user, f: f %>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %>
  </div>
  <div><%= f.submit "Submit" %></div>
<% end %>